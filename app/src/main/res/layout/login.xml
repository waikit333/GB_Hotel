<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/loginbackground" >

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="294dp"
        android:layout_height="106dp"
        app:srcCompat="@drawable/textbox"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="416dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="145dp"
        android:layout_height="127dp"
        app:srcCompat="@drawable/icons8_beach_100"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="146dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="263dp"
        android:layout_height="49dp"
        app:srcCompat="@drawable/golden"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="273dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="294dp"
        android:layout_height="105dp"
        app:srcCompat="@drawable/textbox"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="349dp" />

    <ImageView
        android:id="@+id/imageHelpIcon"
        android:layout_width="39dp"
        android:layout_height="40dp"
        app:srcCompat="@drawable/icons8_help_100"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="501dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="casual"
        android:text="need help ?"
        android:textColor="@color/black"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="510dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="243dp"
        android:layout_height="44dp"
        android:ems="10"
        android:fontFamily="@font/pala"
        android:inputType="textEmailAddress"
        android:text="email"
        android:textColor="#D2AC47"
        tools:layout_editor_absoluteX="84dp"
        tools:layout_editor_absoluteY="372dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="245dp"
        android:layout_height="44dp"
        android:ems="10"
        android:fontFamily="@font/pala"
        android:inputType="textPassword"
        android:text="password"
        android:textColor="#D2AC47"
        tools:layout_editor_absoluteX="84dp"
        tools:layout_editor_absoluteY="440dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LogIn"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="518dp" />

</androidx.constraintlayout.widget.ConstraintLayout>